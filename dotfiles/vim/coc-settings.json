{
	// COMPLETION-PRIORITIES
	// completion=1
	// emmet=2
	// snippets=3
	// words=9
	// browser=10

	// פּ                
	//   <>   	       ﱮ  塞       練蘒
	"suggest.completionItemKindLabels": {
		"method": "  ",
		"function": "  ",
		"variable": "[]",
		"field": "  ",
		"typeParameter": "<>",
		"constant": "  ",
		"class": "  ",
		"interface": "  ",
		"struct": "  ",
		"event": "  ",
		"operator": "  ",
		"module": " פּ ",
		"property": "  ",
		"enum": " 練",
		"reference": "  ",
		"keyword": "  ",
		"file": "  ",
		"folder": " ﱮ ",
		"color": "  ",
		"unit": " 塞 ",
		"snippet": "  ",
		"text": "  ",
		"constructor": "  ",
		"value": "  ",
		"enumMember": "  ",
		"default": "  "
	},

	"codeLens.enable": false,

	"list.indicator": ">",
	"list.selectedSignText": " ",

	//coc-emmet
	"emmet.priority": 2,
	"emmet.showSuggestionsAsSnippets": true,

	//coc-snippets
	"snippets.priority": 3,
	"snippets.shortcut": "SNIP",
	"snippets.enableStatusItem": false,

	//coc-explorer
	"explorer.width": 60,
	"explorer.icon.enableNerdfont": true,
	"explorer.git.showIgnored": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.file.git.showUntrackedFiles": true,
	"explorer.filename.colored.enable": true,
	"explorer.diagnostic.enableSubscriptNumber": true,
	"explorer.git.icon.status.modified": "●",
	"explorer.git.icon.status.added": "✚",
	/*"explorer.git.icon.status.ignored": "!",*/
	"explorer.git.icon.status.deleted": "✖",
	"explorer.git.icon.status.renamed": "ℝ",
	/*"explorer.git.icon.status.untracked": "?",*/
	"explorer.git.icon.status.unmerged": "𝕌",

	//coc-prettier
	"prettier.semi": true,
	"prettier.useTabs": true,
	"prettier.tabWidth": 2,
	"prettier.printWidth": 80,
	"prettier.singleQuote": true,
	"prettier.arrowParens": "avoid",
	"prettier.trailingComma": "es5",
	"prettier.jsxBracketSameLine": false,
	"prettier.jsxSingleQuote": false,
	"prettier.bracketSpacing": true,
	"prettier.disableSuccessMessage": true,
	"prettier.eslintIntegration": true,
	"prettier.tslintIntegration": true,
	"prettier.onlyUseLocalVersion": true,
	"prettier.statusItemText": "Prettier",

	//coc-pairs
	"pairs.enableBackspace": true,
	"pairs.enableCharacters": [
		"(",
		"[",
		"{",
		"<",
		"'",
		"\"",
		"`"
	],

	//coc-git
	"git.signPriority": 2,
	/*"git.addedSign.text": "+",*/
	/*"git.removedSign.text": "-",*/
	/*"git.changedSign.text": "~",*/
	/*"git.changeRemovedSign.text": "≃",*/
	"git.addedSign.hlGroup": "JatGitAdd",
	"git.removedSign.hlGroup": "JatGitDelete",
	"git.changedSign.hlGroup": "JatGitChange",
	"git.changeRemovedSign.hlGroup": "JatGitChange",
	"git.blameUseRealTime": false,
	"git.addGBlameToVirtualText": false,
	"git.virtualTextPrefix": "\t",
	"git.conflict.current.hlGroup": "DiffChange",
	"git.conflict.incoming.hlGroup": "DiffAdd",

	//coc-diagnostics
	"diagnostic.enable": true,
	"diagnostic.enableSign": false,
	"diagnostic.signPriority": 1,
	"diagnostic.errorSign": "ﱢ",
	"diagnostic.warningSign": "ﱢ",
	"diagnostic.infoSign": "ﱢ",
	"diagnostic.hintSign": "ﱢ",
	//"diagnostic.errorSign": " ",
	//"diagnostic.warningSign": " ",
	//"diagnostic.infoSign": "",
	//"diagnostic.hintSign": " ",
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextLines": 3,
	"diagnostic.virtualTextPrefix": " ﱢ ",
	"diagnostic.virtualTextCurrentLineOnly": false,
	"diagnostic.virtualTextLineSeparator": " ● ",
	"diagnostic.enableMessage": "always",
	"diagnostic.messageTarget": "float",
	"diagnostic.format": "%message\n[%source:%code]",
	/*"diagnostic.checkCurrentLine": false,*/

	//markdown-lint
	"markdownlint.config": {
		"no-multiple-blanks": false,
		"blanks-around-headings": false,
		"headings": false,
	},

	//coc-yank
	"yank.priority": 100,

	//coc-zi
	"zi.enable": true,

	//coc-word
	"coc.source.word.enable": true,
	"coc.source.word.shortcut": "10K",
	"coc.source.word.priority": 10,
	"coc.source.word.filetypes": ["text"],

	//coc-browser
	"browser.priority": 10,

	//coc-spell-checker
	"cSpell.enabled": true,
	"cSpell.language": "en",
	"cSpell.showStatus": false,
	"cSpell.diagnosticLevel": "Hint",
	"cSpell.allowCompoundWords": true,
	"cSpell.fixSpellingWithRenameProvider": true,
	"cSpell.userWords": [
		"Conteful",
		"Contentful",
		"UPDATION",
		"inplace",
		"uninteractive",
		"untracked"
	],
	"cSpell.ignoreWords": [
	],
}
