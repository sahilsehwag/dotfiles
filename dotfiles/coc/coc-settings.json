{
	// PRIORITIES:COMPLETION
	// completion=1
	// emmet=2
	// snippets=3
	// words=9
	// browser=10

	//coc-emmet
	"emmet.priority": 2,
	"emmet.showSuggestionsAsSnippets": true,

	//coc-snippets
	"snippets.priority": 3,
	"snippets.shortcut": "SNIP",
	"snippets.enableStatusItem": false,

	//coc-explorer
	"explorer.width": 45,
	"explorer.git.showIgnored": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.file.git.showUntrackedFiles": true,
	"explorer.filename.colored.enable": true,
	"explorer.diagnostic.enableSubscriptNumber": true,
	"explorer.git.icon.status.modified": "‚óè",
	"explorer.git.icon.status.added": "‚úö",
	/*"explorer.git.icon.status.ignored": "!",*/
	"explorer.git.icon.status.deleted": "‚úñ",
	"explorer.git.icon.status.renamed": "‚Ñù",
	/*"explorer.git.icon.status.untracked": "?",*/
	"explorer.git.icon.status.unmerged": "ùïå",

	//coc-prettier
	"prettier.semi": true,
	"prettier.useTabs": true,
	"prettier.tabWidth": 2,
	"prettier.printWidth": 80,
	"prettier.singleQuote": true,
	"prettier.arrowParens": "avoid",
	"prettier.trailingComma": "es5",
	"prettier.jsxBracketSameLine": false,
	"prettier.jsxSingleQuote": false,
	"prettier.bracketSpacing": true,
	"prettier.disableSuccessMessage": true,
	"prettier.eslintIntegration": true,
	"prettier.tslintIntegration": true,
	"prettier.onlyUseLocalVersion": true,
	"prettier.statusItemText": "Prettier",

	//coc-pairs
	"pairs.enableBackspace": true,
	"pairs.enableCharacters": [
		"(",
		"[",
		"{",
		"<",
		"'",
		"\"",
		"`"
	],

	//coc-git
	"git.signPriority": 2,
	/*"git.addedSign.text": "+",*/
	/*"git.removedSign.text": "-",*/
	/*"git.changedSign.text": "~",*/
	/*"git.changeRemovedSign.text": "‚âÉ",*/
	"git.addedSign.hlGroup": "JatGitAdd",
	"git.removedSign.hlGroup": "JatGitDelete",
	"git.changedSign.hlGroup": "JatGitChange",
	"git.changeRemovedSign.hlGroup": "JatGitChange",
	"git.blameUseRealTime": false,
	"git.addGBlameToVirtualText": false,
	"git.virtualTextPrefix": "     ",
	"git.conflict.current.hlGroup": "DiffChange",
	"git.conflict.incoming.hlGroup": "DiffAdd",

	//coc-diagnostics
	"diagnostic.enable": true,
	"diagnostic.enableSign": false,
	"diagnostic.signPriority": 1,
	"diagnostic.errorSign": "Ô±¢",
	"diagnostic.warningSign": "Ô±¢",
	"diagnostic.infoSign": "Ô±¢",
	"diagnostic.hintSign": "Ô±¢",
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextLines": 3,
	"diagnostic.virtualTextPrefix": " Ô±¢ ",
	"diagnostic.virtualTextCurrentLineOnly": false,
	"diagnostic.virtualTextLineSeparator": " ‚óè ",
	"diagnostic.enableMessage": "never",
	"diagnostic.messageTarget": "float",
	"diagnostic.format": "%message\n[%source:%code]",
	/*"diagnostic.checkCurrentLine": false,*/

	//coc-yank
	"yank.priority": 100,

	//coc-zi
	"zi.enable": true,

	//coc-word
	"coc.source.word.enable": true,
	"coc.source.word.shortcut": "10K",
	"coc.source.word.priority": 10,
	"coc.source.word.filetypes": ["text"],

	//coc-browser
	"browser.priority": 9,

	//coc-spell-checker
	"cSpell.enabled": true,
	"cSpell.language": "en",
	"cSpell.showStatus": false,
	"cSpell.diagnosticLevel": "Hint",
	"cSpell.allowCompoundWords": true,
	"cSpell.fixSpellingWithRenameProvider": true,
	"cSpell.userWords": [
		"Untracked"
	],
	"cSpell.ignoreWords": [
	],
}
