#VARIABLES
	EDITOR=vim
	GEDITOR=gvim.exe
	ONEDRIVE=/mnt/c/Users/sehwa/OneDrive
	EXEDIRS=("/mnt/c/Program Files" "/mnt/c/Program Files (x86)" /mnt/c/Users/sehwa/AppData/Roaming /mnt/c/Users/sehwa/AppData/Local)
	VDIRS=(/mnt/c/Users/sehwa/Desktop)
	MDIRS=(/mnt/c/Users/sehwa/Desktop)
	VLC="/mnt/c/Program Files (x86)/VideoLAN/VLC/vlc.exe"
	WMP="/mnt/c/Program Files (x86)/Windows Media Player/wmplayer.exe"
#NAVIGATION
	alias fcd='cd "$(find "`pwd`" -type d -name "*" | fzf)"'
	alias fjh='cd "$(find /home -type d -name "*" | fzf)"'
	alias fjo='cd "$(find $ONEDRIVE -type d -name "*" | fzf)"'
#GENERAL FUNCTIONS
	fd(){
		if [ $1 ] 
		then
			$1 "$(find $2 -type d -name "*" 2> /dev/null | fzf)"
		else
			echo "No command specified"
		fi
	}
	ffh(){
		if [ $1 ] 
		then
			$1 "$(find /home -type f -name "*" | fzf)"
		else
			echo "No command specified"
		fi
	}
	ffo(){
		if [ $1 ] 
		then
			$1 "$(find $ONEDRIVE -type f -name "*" | fzf)"
		else
			echo "No command specified"
		fi
	}
	ff(){
		if [ $1 ] 
		then
			$1 "$(find $2 -type f -name "*" | fzf)"
		else
			echo "No command specified"
		fi
	}
#VIM
	alias fv='$EDITOR "$(find "`pwd`" -type f -name "*" | fzf)"'
	alias fvh='$EDITOR "$(find /home -type f -name "*" | fzf)"'
	alias fvo='$EDITOR "$(find $ONEDRIVE -type f -name "*" | fzf)"'
#VIFM
	alias vifmo='vifm "$(find $ONEDRIVE -type d -name "*" | fzf)"'
	alias vifmh='vifm "$(find /home -type d -name "*" | fzf)"'
	alias vifm.='vifm "$(find "`pwd`" -type d -name "*" | fzf)"'
#TMUX
	#alias fts='tmux switch -t $(tmux list-sessions | awk "{ print $1 }" | sed "s/://" | fzf-tmux)'
	#alias ftw='tmux select-window -t $(tmux list-sessions | awk "{ print $1 }" | sed "s/://" | fzf-tmux)'
#WINDOWS APPLICATION
	alias fg='$GEDITOR "$(find "`pwd`" -type f -name "*" | fzf | sed -e "s/\/mnt\///" | sed -e "s/c/C:/")"' 
	alias fgh='$GEDITOR "$(find /home -type f -name "*" | fzf | sed -e "s/\/mnt\///" | sed -e "s/c/C:/")"'
	alias fgo='$GEDITOR "$(find $ONEDRIVE -type f -name "*" | fzf | sed -e "s/\/mnt\///" | sed -e "s/c/C:/")"'
#APPLICATIONS
	alias launch='"$(find "${EXEDIRS[@]}" -type f -name "*.exe" 2> /dev/null | fzf)"'
