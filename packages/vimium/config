#MAPPINGS
	#BASICS
		#UNMAPPINGS
			unmap gt
			unmap gT
			unmap gf
			unmap ge
			unmap gE
			unmap W
			unmap <<
			unmap >>
			unmap <a-p>
			unmap <a-m>
			unmap <a-f>
			unmap yt
			unmap g0
			unmap g$
			unmap T
			unmap <c-e>
			unmap <c-y>
			unmap gu
			unmap gU
			unmap [[
			unmap ]]
			unmap ^
		#INBUILT
			#TABS
				map wd duplicateTab
				map wn createTab window
				map wi createTab incognito
				map ww moveTabToNewWindow

				map J previousTab
				map K nextTab
				map ( firstTab
				map ) lastTab

				map - moveTabLeft
				map = moveTabRight
				map _ moveTabLeft count=99
				map + moveTabRight count=99

				map <S-Tab> visitPreviousTab

				map r reload
				map R reload hard

				map x removeTab
				map X restoreTab
				map wco closeOtherTabs
				map wch closeTabsOnLeft
				map wcl closeTabsOnRight

				map <C-p> togglePinTab
				map <C-m> toggleMuteTab
				map wma   toggleMuteTab all
				map wmo   toggleMuteTab other

				map <C-Space> Vomnibar.activateTabSelection
			#HINTS
				map gf LinkHints.activateModeWithQueue
				map gF LinkHints.activateModeToOpenInNewForegroundTab
			#EDITING
				map <C-l> Vomnibar.activateEditUrl
				map <C-L> Vomnibar.activateEditUrlInNewTab

				map <C-u> goUp
				map <C-U> goToRoot
			#PAGINATION
				map [ goPrevious
				map ] goNext
			#GROUPS
				map wgn createTab window https://neovimcraft.com https://this-week-in-neovim.org/
				map wge createTab window https://primevideo.com  https://netflix.com/ https://hotstar.com/
		#REMAPS
		#SEARCH
			map sg Vomnibar.activate keyword=github
			map sb Vomnibar.activate keyword=bing
			map sy Vomnibar.activate keyword=youtube
			map sm Vomnibar.activate keyword=google_maps
			map sa Vomnibar.activate keyword=amazon
			map sw Vomnibar.activate keyword=wikipedia
		#SEARCH(NEW)
			map sG Vomnibar.activateInNewTab keyword=gitub
			map sB Vomnibar.activateInNewTab keyword=bing
			map sY Vomnibar.activateInNewTab keyword=youtube
			map sM Vomnibar.activateInNewTab keyword=google_maps
			map sW Vomnibar.activateInNewTab keyword=wikipedia
			map sA Vomnibar.activateInNewTab keyword=amazon
	#SEARCH-ENGINES
		google:      http://www.google.com/search?q=%s                                   Google
		github:      http://github.com/search?q=%s                                       Github
		youtube:     http://www.youtube.com/results?search_query=%s                      Youtube
		wikipedia:   http://www.wikipedia.org/w/index.php?title=Special:Search&search=%s Wikipedia
		google_maps: https://www.google.com/maps?q=%s Google                             maps
		bing:        https://www.bing.com/search?q=%s                                    Bing
		duckduckgo:  https://duckduckgo.com/?q=%s                                        DuckDuckGo
		amazon:      http://www.amazon.com/s/?field-keywords=%s                          Amazon
		dictionary:  http://www.merriam-webster.com/dictionary/%s                        Merriam-Webster
